cQuery += "WHERE B7_FILIAL = '"+xFilial("SB7")+"'"+cEOL
cQuery += "		AND B7_DATA = '"+cData+"'"+cEOL
cQuery += "		AND B7_COD <> ' '"+cEOL
cQuery += "		AND B7_CONTAGE = '002'"+cEOL
cQuery += "		AND B7_DTDIGIT = ' '"+cEOL
