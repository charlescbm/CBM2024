						//				ENDIF
						//			DBSELECTAREA("TRB")
						TRB->NUMSER	   :=	QPK->QPK_NUMSER
						TRB->REVI		:=	cRevi
						
						TRB->FATCO1		:= QP6->QP6_FATCO1
						TRB->TIPCO1		:= QP6->QP6_TIPCO1
						TRB->FATCO2		:= QP6->QP6_FATCO2
