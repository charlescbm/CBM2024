If Li > 63
	CbcGeral()
	nCabRot:= 0
EndIf

// Para imprimir somente uma vez
If nCabRot = 0
	@ ++Li, 00 PSay __PrtFatLine()
	@ ++Li, 00 PSay cCabec2
	@ ++Li, 00 PSay __PrtFatLine()
	nCabRot:= 1
EndIf
	
@ ++Li, 00 PSay IIf( lSH8, SH8->H8_RECURSO, SG2->G2_RECURSO ) + " " + SUBS( SH1->H1_DESCRI, 1, 25 )
@   Li, 33 PSay SG2->G2_FERRAM + " " + Subs( SH4->H4_DESCRI, 1, 20 )
