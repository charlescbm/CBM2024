User Function RH(aParam)
Private cFil  := ""  
Private cBrasMail := ""

Prepare Environment Empresa aParam[1] Filial "01"

ConOut(" ")
ConOut("> RH - Workflow... Inicio: "+Time()) 

CHKFILE("SM0")
DBSelectArea("SM0")
DBSetOrder(1)
DBSeek(aParam[1],.F.)
If !SM0->(Eof()) .AND. SM0->M0_CODIGO == aParam[1]
	cFilAnt	:= SM0->M0_CODFIL		
	RHFUNC(aParam[2])	
EndIf

ConOut("> RH - Workflow... Fim: "+Time())
RESET ENVIRONMENT
Return

/****************************/
Static Function RHFUNC(nOpcao)
/****************************/        
If nOpcao == NIL
   nOpcao := 0          
End
Do Case
   Case nOpcao == 0
   		GPE_EXP()     //Vencimento 1 Experiencia
   Case nOpcao == 1
		GPE_2EXP()    //Vencimento 2 Experiencia
   Case nOpcao == 2
		GPE_ANIVER()  //Lista de Aniversariantes
   Case nOpcao == 3
		GPE_FERIAS1() //F้rias a Vencer  
   Case nOpcao == 4
		GPE_FER2PER() //F้rias Vencidas
End             	
Return		

