User Function DacCta(cNum, cTipo)
Local nPos	:= 1	//Posicao do caracter no texto
Local cRet	:= "" 	//Retorno 
Local lRetTip := If( Upper( Substr( cTipo, 1, 1) ) == 'C', .F., .T. )
For _nA := 1 To len( cNum ) 
	If Upper( Substr( cNum, _nA, 1 ) ) == "-" 
		If lRetTip 
			cRet := AllTrim( Substr(cNum, _nA+1 ) ) 
		Else
			cRet := AllTrim( Substr(cNum, 1, _nA-1 ) ) 
		EndIf
		Exit
    EndIf
Next _nA
Return( cRet ) 